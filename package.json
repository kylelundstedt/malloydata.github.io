{
  "name": "malloydata.github.io",
  "version": "0.0.1",
  "description": "Malloy documentation",
  "type": "module",
  "private": true,
  "scripts": {
    "docs-clean": "rm -rf .jekyll-cache/ && rm -rf _includes/generated/ && rm -rf documentation && rm -rf js/generated/",
    "docs-prebuild": "npm install && npm run docs-clean",
    "docs-build": "npm run docs-prebuild && ts-node --esm _scripts/build_docs/index.ts && bundle exec jekyll build",
    "docs-preprocess": "ts-node --esm _scripts/build_docs/index.ts --watch",
    "docs-postprocess": "wait-on ./_includes/generated/toc.html && bundle exec jekyll serve -l -o",
    "docs-serve": "npm run docs-prebuild && concurrently --kill-others 'npm run docs-preprocess' 'npm run docs-postprocess'",
    "docs-deploy": "npm run docs-prebuild && ts-node --esm _scripts/build_docs/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malloydata/malloydata.github.io.git"
  },
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/malloydata/malloydata.github.io/issues"
  },
  "homepage": "https://github.com/malloydata/malloydata.github.io#readme",
  "devDependencies": {
    "@malloydata/db-bigquery": "^0.0.15",
    "@malloydata/malloy": "^0.0.15",
    "@malloydata/render": "^0.0.15",
    "@types/archiver": "^5.3.1",
    "@types/jsdom": "^20.0.0",
    "archiver": "^5.3.0",
    "concurrently": "^6.2.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^19.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^10.0.1",
    "shiki": "^0.10.1",
    "ts-node": "^10.9.1",
    "unified": "^10.1.2",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13"
  }
}
