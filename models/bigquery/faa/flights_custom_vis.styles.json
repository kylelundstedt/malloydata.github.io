      "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "height": 50,
      "data": [],
      "mark": "area",
      "encoding": {
        "x": {
          "field": "#{1}",
          "type": "temporal",
          "axis": { "grid": false },
          "scale" : {"domain":["2000-01-01","2007-01-01"]},
          "title": null
